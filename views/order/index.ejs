  </head>
  <body>
    

  <nav>
    <a href="/" <%- title === 'Home' ? 'class="active"' : '' %>>Home</a>
    <a href="/womens">Womens</a>
    <a href="/mens">Mens</a>
    <a href="/kids">Kids</a>
    <a href="/products">All Products</a>
    <a id="cart" href="/cart"> View Cart </a>
    <% if (user) { %>
        
        <a href="/logout">LOG OUT</a>
        <!-- <a id="my-account" href="/account">My Account</a><br> -->
    <% } else { %>
      <a href="/auth/google" class="login">LOG IN&nbsp;<img src="https://i.imgur.com/FHjYyi0.png"></a>
    <% } %>
  </nav>
    <h3>Order Details</h3>
        <div id="order-details">
            <ul> 
                <li>     
            <% orders.forEach(function(order) {  %>

                Order #: <%= order._id %> <br> 
                Products: <%= order.products[0].name %> <br> 
                Customer Name:<%= order.customer.name %> <br>
            
                </li>
            <% }); %>
      
            </ul>
        </div>




</body>
</html>

